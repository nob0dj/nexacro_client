    <Script type="xscript5.1"><![CDATA[
this.Button00_onclick = function(obj:nexacro.Button,e:nexacro.ClickEventInfo)
{
	//이름
	var full_name = this.Div00.form.Edit00.value;
	//아이디
	var empl_id = this.Div00.form.MaskEdit00.value;
	//가입일
	var hire_date = this.Div00.form.Calendar00.value;
	//부서코드
	var dept_cd = this.Div00.form.Combo00.value;
	//직급코드
	var pos_cd = this.Div00.form.ListBox00.value;
	//급여
	var salary = this.Div00.form.MaskEdit00.value;
	//성별
	var gender = this.Div00.form.Radio00.value;
	//결혼여부
	var married = this.Div00.form.CheckBox00.value;
	//메모
	var memo = this.Div00.form.TextArea00.value;
	
	var result = "이름 : " + full_name + "\n"
			   + "아이디 : " + empl_id + "\n"
			   + "입사일 : " + hire_date + "\n"
			   + "부서코드 : " + dept_cd + "\n"
			   + "직급코드 : " + pos_cd + "\n"
			   + "급여 : " + salary + "\n"
			   + "성별 : " + gender + "\n"
			   + "결혼여부 : " + married + "\n"
			   + "메모 : " + memo + "\n";
			   
	alert(result);
	
	//초기화
	this.reload();
};
